{
	"rust-analyzer.diagnostics.disabled": [
		"macro-error",
		"unresolved-import",
		"unresolved-macro"
	],
	"rust-analyzer.linkedProjects": [
		"ubiquitous-data-server/Cargo.toml",
		"ubiquitous-viz-server/Cargo.toml",
		"ubiquitous-auth-server/Cargo.toml"
	],
	"gopls": {
		"experimentalWorkspaceModule": true
	},
	"cSpell.words": [
		"Actix",
		"Cyberbrick",
		"Lettre",
		"VARCHAR",
		"datetime",
		"lura",
		"postgres",
		"rustup"
	],
	"files.watcherExclude": {
		"**/target": true
	}
}
