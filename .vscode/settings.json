{
	"rust-analyzer.diagnostics.disabled": [
		"macro-error",
		"unresolved-import",
		"unresolved-macro"
	],
	"rust-analyzer.linkedProjects": [
		"ubiquitous-data-server/Cargo.toml",
		"ubiquitous-viz-server/Cargo.toml",
		"ubiquitous-auth-server/Cargo.toml"
	],
	"gopls": {
		"experimentalWorkspaceModule": true
	},
	"java.trace.server": "verbose",
	"cSpell.words": [
		"Actix",
		"Configurer",
		"Cyberbrick",
		"datetime",
		"FILEBROWSER",
		"Lettre",
		"lura",
		"pfms",
		"postgres",
		"rustup",
		"Servlet",
		"typeorm",
		"upsert",
		"VARCHAR"
	],
	"files.watcherExclude": {
		"**/target": true
	},
	"python.formatting.provider": "black",
	"java.configuration.updateBuildConfiguration": "automatic"
}
