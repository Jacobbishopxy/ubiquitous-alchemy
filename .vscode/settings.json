{
	"rust-analyzer.diagnostics.disabled": [
		"macro-error",
		"unresolved-import",
		"unresolved-macro"
	],
	"rust-analyzer.linkedProjects": [
		"ubiquitous-data-server/Cargo.toml",
		"ubiquitous-viz-server/Cargo.toml",
		"ubiquitous-auth-server/Cargo.toml"
	],
	"gopls": {
		"experimentalWorkspaceModule": true
	},
	"cSpell.words": [
		"Actix",
		"Cyberbrick",
		"datetime",
		"FILEBROWSER",
		"Lettre",
		"lura",
		"postgres",
		"rustup",
		"VARCHAR"
	],
	"files.watcherExclude": {
		"**/target": true
	},
	"grammarly.overrides": [
		{
			"files": ["**/documentation/Documentatiaon-En.md"],
			"config": {
				"audience": "general",
				"dialect": "american",
				"domain": "technical",
				"emotions": ["neutral"],
				"goals": ["inform", "describe"]
			}
		}
	]
}
