{
	"version": {{ .service.version }},
	"name": "{{ .service.name }}",
	"port": {{ .service.port }},
	"timeout": "{{ .service.timeout }}",
	"cache_ttl": "{{ .service.cache_ttl }}",
	"endpoints": [
		{
			"endpoint": "/gateway/api/cfg/check_connection",
			"method": "POST",
			"output_encoding": "no-op",
			"backend": [
				{
					"encoding": "no-op",
					"method": "POST",
					"url_pattern": "/api/cfg/check_connection",
					"host": [
						"{{ .host.host }}:{{ .host.server_data_port }}"
					]
				}
			]
		},
		{
			"endpoint": "/gateway/api/cfg/conn",
			"output_encoding": "no-op",
			"backend": [
				{
					"encoding": "no-op",
					"url_pattern": "/api/cfg/conn",
					"host": [
						"{{ .host.host }}:{{ .host.server_data_port }}"
					]
				}
			]
		},
		{
			"endpoint": "/gateway/api/cfg/conn",
			"method": "POST",
			"output_encoding": "no-op",
			"backend": [
				{
					"encoding": "no-op",
					"method": "POST",
					"url_pattern": "/api/cfg/conn",
					"host": [
						"{{ .host.host }}:{{ .host.server_data_port }}"
					]
				}
			]
		},
		{
			"endpoint": "/gateway/api/cfg/conn",
			"method": "PUT",
			"querystring_params": ["db_id"],
			"output_encoding": "no-op",
			"backend": [
				{
					"encoding": "no-op",
					"method": "PUT",
					"url_pattern": "/api/cfg/conn",
					"host": [
						"{{ .host.host }}:{{ .host.server_data_port }}"
					]
				}
			]
		},
		{
			"endpoint": "/gateway/api/cfg/conn",
			"method": "DELETE",
			"querystring_params": ["db_id"],
			"output_encoding": "no-op",
			"backend": [
				{
					"encoding": "no-op",
					"method": "DELETE",
					"url_pattern": "/api/cfg/conn",
					"host": [
						"{{ .host.host }}:{{ .host.server_data_port }}"
					]
				}
			]
		},
		{
			"endpoint": "/gateway/api/auth/invitation",
			"method": "POST",
			"output_encoding": "no-op",
			"backend": [
				{
					"encoding": "no-op",
					"method": "POST",
					"url_pattern": "/api/invitation",
					"host": [
						"{{ .host.host }}:{{ .host.server_auth_port }}"
					]
				}
			]
		},
		{
			"endpoint": "/gateway/api/auth/register/{id}",
			"method": "GET",
			"output_encoding": "no-op",
			"backend": [
				{
					"encoding": "no-op",
					"url_pattern": "/api/register/{id}",
					"host": [
						"{{ .host.host }}:{{ .host.server_auth_port }}"
					]
				}
			]
		},
		{
			"endpoint": "/gateway/api/auth",
			"method": "POST",
			"output_encoding": "no-op",
			"backend": [
				{
					"encoding": "no-op",
					"url_pattern": "/api/auth",
					"host": [
						"{{ .host.host }}:{{ .host.server_auth_port }}"
					]
				}
			]
		},
		{
			"endpoint": "/gateway/api/auth",
			"method": "GET",
			"output_encoding": "no-op",
			"headers_to_pass": ["Cookie"],
			"backend": [
				{
					"encoding": "no-op",
					"url_pattern": "/api/auth",
					"host": [
						"{{ .host.host }}:{{ .host.server_auth_port }}"
					]
				}
			]
		},
		{
			"endpoint": "/gateway/api/auth",
			"method": "DELETE",
			"output_encoding": "no-op",
			"headers_to_pass": ["Cookie"],
			"backend": [
				{
					"encoding": "no-op",
					"url_pattern": "/api/auth",
					"host": [
						"{{ .host.host }}:{{ .host.server_auth_port }}"
					],
					"headers_to_pass": ["Cookie"]
				}
			]
		}
	]
}
